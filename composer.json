{
    "name": "healyhatman/keypay-php",
    "description": "Package to access the KeyPay API",
    "keywords": [
        "healyhatman",
        "laravel",
        "keypay-php"
    ],
    "homepage": "https://github.com/healyhatman/keypay-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamron Brooks",
            "email": "kamron@skyspider.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "league/oauth2-client": "1.*|2.*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "guzzlehttp/psr7": "^1.5|^1.6",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Healyhatman\\Skeleton\\": "src",
            "Healyhatman\\Skeleton\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Healyhatman\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
    "minimum-stability": "dev",
    "prefer-stable": true
}
